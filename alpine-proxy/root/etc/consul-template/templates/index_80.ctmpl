<!DOCTYPE HTML>
<html>
 <head>
  <meta charset="utf-8">
  <title>Proxy</title>
 </head>
 <body>
{{ range services }}
  {{ $is_public := .Tags | contains "public" }}
  {{ $is_private := .Tags | contains "private" }}
  {{ if or $is_public $is_private }}
    {{ $service:=.Name }}
    {{ range service $service "any" }}
      {{ if eq .Status "critical"}}
      {{.Name}} (status: critical)
      {{ else }}
<p><a href="{{.Name}}">{{.Name}}</a></p>
      {{ end }}
    {{ end }}
  {{ end }}
{{ end }}
 </body>
</html>
